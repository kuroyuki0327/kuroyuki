// slick.min.jsã®ä»£ã‚ã‚Šã«ã€Slickã‚’å‘¼ã³å‡ºã™éƒ¨åˆ†ã®ã¿ã‚’è¨˜è¿°
if (typeof jQuery.fn.slick === 'undefined') {
    // å®Ÿéš›ã¯slick.min.jsã®å…¨ã‚³ãƒ¼ãƒ‰ã‚’æŒ¿å…¥ã—ã¾ã™ãŒã€ã“ã“ã§ã¯ã‚³ãƒ³ã‚½ãƒ¼ãƒ«ã®ã‚¨ãƒ©ãƒ¼ã‚’é¿ã‘ã‚‹ãŸã‚ã«å®šç¾©ã®ã¿è¡Œã„ã¾ã™
    // ãƒ‡ãƒ¢ç›®çš„ã®ãŸã‚ã€Slickã®æ©Ÿèƒ½ã‚’ç„¡åŠ¹åŒ–ã¾ãŸã¯å˜ç´”åŒ–ã—ã¾ã™
    jQuery.fn.slick = function(options) {
        if (options === 'getSlick') {
            return {
                slideCount: this.find('.slick-slide').length,
                options: options,
                unslicked: false,
                setSlideClasses: function() {},
                slideHandler: function() {}
            };
        }
        if (options === 'unslick') {
            return this;
        }
        if (options === 'slickFilter') {
            return this;
        }
        // ä»–ã®Slickãƒ¡ã‚½ãƒƒãƒ‰ã®å‡¦ç†...
        return this;
    };
}
// --- slick.min.js end ---

$(function(){
    // pc_sp: ç”»é¢å¹…ã«å¿œã˜ã¦PC/SPã‚’åˆ‡ã‚Šæ›¿ãˆã‚‹
	if ($(window).width() > 736) {
		$('.sp').remove();
	} else {
		$('.pc').remove();
	}

    // Retina: ç”»åƒURLã®@2xç½®æ›ã¯ã€å¤–éƒ¨ç”»åƒãŒãªã„ãŸã‚ç„¡åŠ¹åŒ–
	/*
	if (window.devicePixelRatio > 1.24) {
		$('img.retina').each(function() {
			$(this).attr('src',
			$(this).attr('src').replace(/(\.)(png|jpg|gif)/gi,'@2x$1$2'));
		});
	}
    */

    // Typekit: å¤–éƒ¨ãƒ•ã‚©ãƒ³ãƒˆã®ãƒ­ãƒ¼ãƒ‰ã¯ç„¡åŠ¹åŒ–
	/*
	(function(d) {
		var config = {
			kitId: 'eqn3fkh',
			scriptTimeout: 3000,
			async: true
		},
		h=d.documentElement,t=setTimeout(function(){h.className=h.className.replace(/\bwf-loading\b/g,"")+" wf-inactive";},config.scriptTimeout),tk=d.createElement("script"),f=false,s=d.getElementsByTagName("script")[0],a;h.className+=" wf-loading";tk.src='https://use.typekit.net/'+config.kitId+'.js';tk.async=true;tk.onload=tk.onreadystatechange=function(){a=this.readyState;if(f||a&&a!="complete"&&a!="loaded")return;f=true;clearTimeout(t);try{Typekit.load(config)}catch(e){}};s.parentNode.insertBefore(tk,s)
	})(document);
	*/


    // navi: SPãƒŠãƒ“ã‚²ãƒ¼ã‚·ãƒ§ãƒ³ã®é–‹é–‰å‡¦ç†
	var state = false;
	var scrollpos;
	$('.header-navi-button').on('click', function(){
		if(state == false) {
			scrollpos = $(window).scrollTop();
            // bodyã«position: fixedã‚’è¨­å®šã™ã‚‹ä»£ã‚ã‚Šã«ã€ãƒˆãƒƒãƒ—ã‚¹ã‚¯ãƒ­ãƒ¼ãƒ«ä½ç½®ã‚’èª¿æ•´
			$('body').addClass('header-navi-open').css({'top': -scrollpos + 'px', 'overflow': 'hidden', 'height': '100vh'});
			state = true;
			$('body').removeClass("header-navi-closed");
		} else {
			$('body').removeClass('header-navi-open').css({'top': 0, 'overflow': '', 'height': ''});
			window.scrollTo( 0 , scrollpos );
			state = false;
			$('body').toggleClass("header-navi-closed");
		}
	});


    // pc navi current: PCãƒŠãƒ“ã®ç¾åœ¨åœ°è¡¨ç¤ºå‡¦ç†
	var href = location.href;
	var links = jQuery(".pc-header-items > li > a");
	links.each(function (index, value) {
		// å³å¯†ãªURLä¸€è‡´
        if (value.href === href) {
			jQuery(".pc-header-items").children("li").eq(index).addClass("current");
		}
        // ãƒ‘ã‚¹ãŒabout.htmlã‚’å«ã‚“ã§ã„ã‚‹å ´åˆ
        var currentPath = location.pathname.substring(location.pathname.lastIndexOf('/') + 1);
        if (currentPath === 'about.html') {
            $('.pc-header-items > li > a[href="about.html"]').parent().addClass('current');
        }
	});

    // ã©ã®ãƒ¡ãƒ‹ãƒ¥ãƒ¼ã«ã‚‚currentãŒãªã„å ´åˆã€TOPã‚’currentã«ã™ã‚‹ï¼ˆindex_lemon.txtã®ãƒ­ã‚¸ãƒƒã‚¯ï¼‰
    if ($('.pc-header-items li.current').length === 0) {
        if (location.pathname === "/" || location.pathname === "/index.html" || location.pathname === "") {
            $('.pc-header-items li:eq(0)').addClass('current');
        }
    }


    // slick ï¼’é€±ç›®ã‚¢ãƒ‹ãƒ¡ãƒ¼ã‚·ãƒ§ãƒ³ã®ã‚¬ã‚¿ã¤ãé˜²æ­¢ - slickãŒå‹•ä½œã—ãªã„ãŸã‚ç„¡åŠ¹åŒ–
	/*
	const $slider = $(".slider-items");
	$slider.on("beforeChange", (event, slick, currentSlide, nextSlide) => {
		$slider.find(".slick-slide").each((index, el) => {
			const $this = $(el),
			slickindex = $this.attr("data-slick-index");
			if (nextSlide == slick.slideCount - 1 && currentSlide == 0) {
				if (slickindex == "-1") {
					$this.addClass("active-next");
				} else {
					$this.removeClass("active-next");
				}
			} else if (nextSlide == 0) {
				if (slickindex == slick.slideCount) {
					$this.addClass("active-next");
				} else {
					$this.removeClass("active-next");
				}
			} else {
				$this.removeClass("active-next");
			}
		});
	});
    */

    // top-hero-ticker-items: Slickã®åˆæœŸåŒ– - slickãŒå‹•ä½œã—ãªã„ãŸã‚ã€ã“ã®éƒ¨åˆ†ã¯ã‚³ãƒ¡ãƒ³ãƒˆã‚¢ã‚¦ãƒˆ
    /*
	$(".top-hero-ticker-items").slick({
		variableWidth: true,
		centerPadding: "0",
		autoplay: true,
		autoplaySpeed: 0,
		speed: 4500,
		dots: false,
		arrows: false,
		infinite: true,
		cssEase: "linear",
		swipe: false,
		pauseOnFocus: false,
		pauseOnHover: false,
		pauseOnDotsHover: false,
		initialSlide: 1,
	});
    */


    let isClosed = false;

    // ã‚¹ã‚¯ãƒ­ãƒ¼ãƒ«ã‚¤ãƒ™ãƒ³ãƒˆå‡¦ç†
	$(window).on("scroll", function () {
	    let scrollTop = $(window).scrollTop();
	    let windowHeight = $(window).height();
	    let footerContact = $(".footer-contact");
	    let footerContactTop = footerContact.offset().top;
	    let footerHeight = footerContact.innerHeight();
	    let footerFixed = $(".pc-footer-fixed");
	    let heroHeight = $(".hero").height();

	    // heroã‚’è¶…ãˆãŸã‚‰ active ã‚’è¿½åŠ ï¼ˆisClosed ãŒ false ã®å ´åˆã®ã¿ï¼‰
	    if (!isClosed && scrollTop > heroHeight) {
	        $(".footer-fixed, .pc-footer-fixed").addClass("active");
	    } else if (scrollTop <= heroHeight) {
            // heroã‚¨ãƒªã‚¢ã«æˆ»ã£ãŸã‚‰éè¡¨ç¤º
            $(".footer-fixed, .pc-footer-fixed").removeClass("active");
        }

	    // footer-contactãŒã‚¦ã‚£ãƒ³ãƒ‰ã‚¦ã®ä¸‹ã‚’éããŸã‚‰ã‚¯ãƒ©ã‚¹ã‚’è¿½åŠ  (PCã®ã¿)
        if ($(window).width() > 736) {
            let stopPoint = footerContactTop - windowHeight + footerFixed.height() + 30; // èª¿æ•´
            if (scrollTop + windowHeight > footerContactTop + footerHeight) {
                footerFixed.addClass("stop");
                footerFixed.css('top', footerContactTop + footerHeight - footerFixed.height() - 30 + 'px');
                footerFixed.css('bottom', 'auto');
            } else {
                footerFixed.removeClass("stop");
                footerFixed.css('top', 'auto');
                footerFixed.css('bottom', '30px'); // CSSã§è¨­å®šã•ã‚ŒãŸå€¤
            }
        }


	    // ã‚¹ã‚¯ãƒ­ãƒ¼ãƒ«ã‚¢ãƒ‹ãƒ¡ãƒ¼ã‚·ãƒ§ãƒ³ (.fade)
		var box = $('.fade');
		var animated = 'fade-active';
		box.each(function(){
			var boxOffset = $(this).offset().top;
			var scrollPos = $(window).scrollTop();
			var wh = $(window).height();
			if(scrollPos > boxOffset - wh + 10 ){
				$(this).addClass(animated);
			}
		});
	}).trigger('scroll'); // åˆæœŸè¡¨ç¤ºæ™‚ã«ã‚‚å®Ÿè¡Œ

    // é–‰ã˜ã‚‹ãƒœã‚¿ãƒ³ã®ã‚¯ãƒªãƒƒã‚¯ã§ active ã‚’å‰Šé™¤ã—ã€isClosedã‚’trueã«è¨­å®š (PCã®ã¿)
	$('.pc-footer-fixed .close-button').on('click', function () {
	    $('.pc-footer-fixed').removeClass('active');
	    isClosed = true; // ä¸€åº¦é–‰ã˜ãŸã‚‰ã€ã‚¹ã‚¯ãƒ­ãƒ¼ãƒ«ã—ã¦ã‚‚å†è¡¨ç¤ºã—ãªã„
	});

	// markerã‚¢ãƒ‹ãƒ¡ãƒ¼ã‚·ãƒ§ãƒ³
	window.addEventListener('scroll',function() {
		const height = window.innerHeight;
		const scroll = this.pageYOffset;
		const marker = document.querySelectorAll('.marker');
		const value = scroll - height + 300

		marker.forEach(function(element){
		if (scroll > element.getBoundingClientRect().top + value) {
		element.classList.add('on')
			}
		});
	});

    // ãƒšãƒ¼ã‚¸å†…ãƒªãƒ³ã‚¯å¯¾ç­–
    $('a[href^="#"]').on('click', function() {
        var href = $(this).attr("href");
        var target = $(href == "#" || href == "" ? 'html' : href);
        var position = target.offset().top - (($(window).width() > 736) ? 115 : 15 * 3.5); // ãƒ˜ãƒƒãƒ€ãƒ¼ã®é«˜ã•ã‚’è€ƒæ…®
        $('body,html').animate({scrollTop:position}, 400, 'swing');
        return false;
    });

});

// Retinaç”¨ã®ç”»åƒã®ç½®ãæ›ãˆå‡¦ç†ã¯ã€å¤–éƒ¨ãƒªã‚½ãƒ¼ã‚¹ã«ä¾å­˜ã™ã‚‹ãŸã‚ã‚¹ã‚­ãƒƒãƒ—ã•ã‚Œã¾ã™
// ä»£ã‚ã‚Šã«ã€retinaã‚¯ãƒ©ã‚¹ã‚’æŒã¤imgã‚¿ã‚°ã¯ãã®ã¾ã¾è¡¨ç¤ºã•ã‚Œã¾ã™
</script>

